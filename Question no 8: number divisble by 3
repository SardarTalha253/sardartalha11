numbers = [5, 12, 7, 18, 9]
div3 = even = odd = 0

for num in numbers:
    if num % 3 == 0:
        print(f"{num} divisible by 3")
        div3 += 1
    elif num % 2 == 0:
        print(f"{num} even but not divisible by 3")
        even += 1
    else:
        print(f"{num} odd and not divisible by 3")
        odd += 1

print(f"Divisible by 3: {div3}, Even: {even}, Odd: {odd}")
